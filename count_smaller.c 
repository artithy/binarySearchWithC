#include <stdio.h>

int countSmaller(int arr[], int n, int target)
{
    int low = 0, high = n - 1;

    while (low <= high)
    {
        int mid = (low + high) / 2;

        if (arr[mid] < target)
            low = mid + 1;
        else
            high = mid - 1;
    }
    return low;
}

int main()
{
    int arr[] = {1, 3, 5, 7, 9};
    int n = sizeof(arr) / sizeof(arr[0]);
    int target = 6;

    int result = countSmaller(arr, n, target);

    printf("Number of elements less than %d = %d\n", target, result);

    return 0;
}
